model School {
    id               String   @id @default(cuid())
    name             String
    abbreviation     String   @unique
    emailFormats     String[]
    studentIdFormat  Json?
    users            User[]
    shops            Shop[]
    instagramAccount String?
    websiteUrl       String?
    isLimited        Boolean  @default(false)

    @@index([abbreviation])
}
